{
  "$schema": "http://json.schemastore.org/prettierrc",
  "singleQuote": false,
  "semi": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "arrowParens": "always",
  "tabWidth": 2,
  "useTabs": false,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": true
}
